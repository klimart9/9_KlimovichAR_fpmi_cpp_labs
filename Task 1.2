#include <iostream>
int main() {
	int Number,c;
	std::cout << "Number >= 1\n";
	std::cin >> Number;
	if (Number >= 1) {
		int c = 1;
		int sum = 0;
		int odd = 1;
		for (int i = 1; i <= Number; i++) {
			if (i % 2 == 0) { sum += i; }
			else { odd *= i; }
		}
		std::cout << "Sum: " << sum << std::endl;
		std::cout << "Umnojenie:" << odd << std::endl;
	}
	else {
		std::cout << "incorrect" << std::endl;
		return 0;
	}
}
