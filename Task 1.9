#include <iostream>


int main() {
    int num;
    std::cout <<"Chetirex znacnoe: ";
    std::cin >> num;

    if (num < 1000 || num > 9999) {
        std::cout << "Incorrect" << std::endl;
        return 0;
    }

    int first = num / 1000;
    int second = (num / 100) % 10;
    int third = (num / 10) % 10;
    int fourth = num % 10;

    if (first == fourth && second == third) {
        std::cout << "Palindrom." << std::endl;
    }
    else {
        std::cout << "Ne palindorom" << std::endl;
    }

    return 0;
}
